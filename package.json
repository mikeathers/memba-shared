{
  "name": "@memba-labs/shared",
  "version": "0.4.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dev": "env-cmd -f ./env/.env.development next build",
    "build:prod": "env-cmd -f ./env/.env.production next build",
    "open-next-build:dev": "open-next build --build-command 'yarn build:dev'",
    "open-next-build:prod": "open-next build --build-command 'yarn build:prod'",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --fix --ext ts,tsx",
    "prettier": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check",
    "prettier:fix": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check --write",
    "prepare": "husky install",
    "test": "jest src --silent",
    "release": "standard-version",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch",
    "pre-release-alpha": "standard-version --prerelease alpha",
    "pre-release-beta": "standard-version --prerelease beta",
    "custom:build": "next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o deployment/build"
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.6.2",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "amazon-cognito-identity-js": "^6.3.3",
    "axios": "^1.4.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "formik": "^2.4.3",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jamesacarr/eslint-formatter-github-actions": "^0.2.0",
    "@jest/types": "^29.5.0",
    "@storybook/addon-essentials": "^7.4.0-alpha.0",
    "@storybook/addon-interactions": "^7.4.0-alpha.0",
    "@storybook/addon-links": "^7.4.0-alpha.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0-alpha.0",
    "@storybook/nextjs": "^7.4.0-alpha.0",
