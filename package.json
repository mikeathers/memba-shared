{
  "name": "@memba-labs/shared",
  "version": "0.20.0",
  "license": "MIT",
  "author": "mikeathers",
  "description": "This is a shared utils package for [Memba](https://memba.co.uk/).",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup --config ./rollup.config.js",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --fix --ext ts,tsx",
    "prettier": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check",
    "prettier:fix": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check --write",
    "prepare": "husky install",
    "test": "jest src --silent",
    "release": "standard-version",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch",
    "pre-release-alpha": "standard-version --prerelease alpha",
    "pre-release-beta": "standard-version --prerelease beta"
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.6.2",
    "axios": "^1.4.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "amazon-cognito-identity-js": "^6.3.3",
    "encoding": "^0.1.13",
